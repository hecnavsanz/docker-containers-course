$TTL    604800
@       IN      SOA     {{ inventory_hostname }}. admin.{{ domain }}. (
                              3         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
; name servers - NS records
       IN      NS      {{ gitlabsrv_hostname }}.{{ domain }}.
; name servers - A records
{{ gitlabsrv_hostname }}.{{ domain }}.         IN      A       {{ gitlabsrv_pub_ip }}
{{ gitlabsrv_hostname }}-priv.{{ domain }}.    IN      A       {{ gitlabsrv_priv_ip }}
; others - A records
{{ gitlabrnr_hostname }}.{{ domain }}.        IN      A       {{ gitlabrnr_pub_ip }}
{{ gitlabrnr_hostname }}-priv.{{ domain }}.   IN      A       {{ gitlabrnr_priv_ip }}
