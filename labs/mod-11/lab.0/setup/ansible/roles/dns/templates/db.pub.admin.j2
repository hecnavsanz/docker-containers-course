$TTL    604800
@       IN      SOA     {{ inventory_hostname }}. admin.{{ domain }}. (
                              3         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
; name servers - NS records
       IN      NS      {{ gitlabsrv_hostname }}.{{ domain }}.
; all - PTR records
{{ gitlabrnr_pub_ip.split('.')[3] }}       IN     PTR    {{ gitlabrnr_hostname }}.{{ domain }}.  
{{ gitlabsrv_pub_ip.split('.')[3] }}        IN     PTR    {{ gitlabsrv_hostname }}.{{ domain }}.
